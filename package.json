{
  "name": "automacao_selenium_origin",
  "version": "1.0.0",
  "description": "Automação de testes com Selenium",
  "homepage": "https://github.com/lteixei/Automacao_Selenium_Origin#readme",
  "bugs": {
    "url": "https://github.com/lteixei/Automacao_Selenium_Origin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lteixei/Automacao_Selenium_Origin.git"
  },
  "license": "ISC",
  "author": "Leonardo Teixeira",
  "type": "commonjs",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
  "test": "cypress run --reporter mochawesome",
  "test:run": "cypress run  --reporter mochawesome",
  "merge:reports": "npx mochawesome-merge cypress/reports/mochawesome/*.json > mochawesome.json",
  "generate:report": "npx marge mochawesome.json -f report -o mochawesome-report",
  "allure:run": "set CYPRESS_ALLURE=true && cypress run",
  "allure:chrome": "set CYPRESS_ALLURE=true && cypress run --browser chrome",
  "allure:repo": "allure generate allure-results --clean -o allure-report",
  "allure:clea": "rm -r allure-results/ allure-report cypress/screenshots true"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.34.1",
    "cypress": "^14.5.1",
    "cypress-iframe": "^1.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
